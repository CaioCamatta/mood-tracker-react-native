{
  "expo": {
    "name": "Quick Journal",
    "slug": "mood-journal",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "notification": {
      "icon": "./assets/notification-48px.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F65058"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.CaioCoelho.moodJournal",
      "buildNumber": "1.0.1"
    },
    "android": {
      "package": "com.CaioCoelho.moodJournal",
      "versionCode": 2
    },
    "androidNavigationBar":{
      "visible": "sticky-immersive"
    }
  }
}
